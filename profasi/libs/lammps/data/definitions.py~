sigmas = {}
with open('data/sigmas.dat') as filein:
    for line in filein:
        line = line.split()
        sigmas[line[0]] = float(line[1])

lambdas = {}
with open('data/lambdas.dat') as filein:
    for line in filein:
        line = line.split()
        lambdas[line[0]] = float(line[1])

