# Multiprotein complexes

units       real
dimension   3
boundary    p p p
atom_style  full

bond_style  harmonic
pair_style  ljlambda 0.1 0.0 35.0
dielectric  80.0

read_data   data.data

# pairwise coefficients

bond_coeff          1     9.600     3.8

$pair_coeff

special_bonds fene
neighbor  3.5 multi
neigh_modify  every 10 delay 0

comm_style    tiled
timestep      $dt

velocity      all create $temp 1234
fix           fxnve  all nve
fix           fxlange all langevin $temp $temp $save 12345

dump          xtc_file all xtc 10000 xtc_traj.xtc
dump_modify   xtc_file unwrap no

run           $t
